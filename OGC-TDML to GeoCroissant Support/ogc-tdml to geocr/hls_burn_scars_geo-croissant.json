      "field": [
        {
          "@type": "cr:Field",
          "@id": "hls_burn_scars/image",
          "name": "hls_burn_scars/image",
          "description": "Satellite imagery with multiple spectral bands",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "tiff-files"
            },
            "extract": {
              "fileProperty": "fullpath"
            },
            "transform": {
              "regex": ".*_merged\\.tif$"
            }
          },
          "geocr:bandConfiguration": {
            "@type": "geocr:BandConfiguration",
            "geocr:totalBands": 6,
            "geocr:bandNameList": [
              "Blue",
              "Green",
              "Red",
              "NIR",
              "SWIR1",
              "SWIR2"
            ]
          }
        },
        {
          "@type": "cr:Field",
          "@id": "hls_burn_scars/mask",
          "name": "hls_burn_scars/mask",
          "description": "Mask annotations with values representing different classes",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "tiff-files"
            },
            "extract": {
              "fileProperty": "fullpath"
            },
            "transform": {
              "regex": ".*\\.mask\\.tif$"
            }
          },
          "geocr:bandConfiguration": {
            "@type": "geocr:BandConfiguration",
            "geocr:totalBands": 1,
            "geocr:bandNameList": [
              "mask"
            ]
          }
        }
      ]
    }
